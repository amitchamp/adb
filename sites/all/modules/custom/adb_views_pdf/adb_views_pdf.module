<?php

/**
 * @file
 * A global views field that allows you to add pdf download link of a current page.
 */

/**
 * Implements hook_views_api().
 */
function adb_views_pdf_views_api() {
  return array(
    'api' => 3,
    'path' => drupal_get_path('module', 'adb_views_pdf') . '/includes/views',
  );
}
